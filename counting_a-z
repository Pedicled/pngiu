#include<stdio.h>
#include<stdlib.h>
#define N 10
/* writting a code for print level picture that rate of a-z&&A-Z */
main()
{
	char a[N];
	int i,b[26];
	gets(a);
	for(i=0;i<26;i++)
		b[i] = 0;
	for(i=0;a[i]!='\0';i++)
	{
		if(a[i]>='a'&&a[i]<='z')
			b[a[i]-'a']++;
		if(a[i]>='A'&&a[i]<='Z')
			b[a[i]-'A']++;
	}
	for(i=0;i<26;i++)
	{
		if(b[i]==0) continue;
		printf("the rate of %c = %d\n",i+'a',b[i]);
	}
	system("pause");
}
